<?xml version="1.0" encoding="UTF-8"?>
<local:AdminPageXaml xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	x:Class="SportRankerMatchOn.Shared.AdminPage" xmlns:local="clr-namespace:SportRankerMatchOn.Shared;assembly=SportRankerMatchOn">
	<local:AdminPageXaml.Content>
		<StackLayout Padding="40" Spacing="20" VerticalOptions="Center">
			<Button x:Name="btnLeagues" Text="Leagues" Style="{StaticResource buttonStyle}"
				IsEnabled="{Binding IsBusy, Converter={x:Static local:InverseBoolConverter.Instance}}" />
			<Button x:Name="btnAthletes" Text="Athletes" Style="{StaticResource buttonStyle}"
				IsEnabled="{Binding IsBusy, Converter={x:Static local:InverseBoolConverter.Instance}}" />
		</StackLayout>
	</local:AdminPageXaml.Content>
</local:AdminPageXaml>