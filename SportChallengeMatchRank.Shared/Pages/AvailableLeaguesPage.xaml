<?xml version="1.0" encoding="UTF-8"?>
<local:AvailableLeaguesXaml xmlns="http://xamarin.com/schemas/2014/forms"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	x:Class="SportChallengeMatchRank.Shared.AvailableLeaguesPage"
	xmlns:local="clr-namespace:SportChallengeMatchRank.Shared;assembly=SportChallengeMatchRank">
	<local:AvailableLeaguesXaml.Content>
		<local:LeagueListView x:Name="list" ItemsSource="{Binding Leagues}" RefreshCommand="{Binding GetAvailableLeaguesCommand}" ShowChallengesButton="false"
			IsRefreshing="{Binding IsBusy, Mode=OneWay}" />
	</local:AvailableLeaguesXaml.Content>

</local:AvailableLeaguesXaml>