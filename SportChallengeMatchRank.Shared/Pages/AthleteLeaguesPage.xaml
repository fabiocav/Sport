<?xml version="1.0" encoding="UTF-8"?>
<local:AthleteLeaguesXaml xmlns="http://xamarin.com/schemas/2014/forms"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	x:Class="SportChallengeMatchRank.Shared.AthleteLeaguesPage"
	xmlns:local="clr-namespace:SportChallengeMatchRank.Shared;assembly=SportChallengeMatchRank">
	<local:AthleteLeaguesXaml.Content>
		<local:LeagueListView x:Name="list" ItemsSource="{Binding Leagues}" RefreshCommand="{Binding GetLeaguesCommand}" IsRefreshing="{Binding IsBusy}" />
	</local:AthleteLeaguesXaml.Content>

	<local:AthleteLeaguesXaml.ToolbarItems>
		<ToolbarItem x:Name="btnJoin" Text="Join" />
	</local:AthleteLeaguesXaml.ToolbarItems>

</local:AthleteLeaguesXaml>